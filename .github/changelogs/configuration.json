{
  "categories": [
    {
      "title": "## 이슈 해결 ✨",
      "labels": ["✨resolved"]
    },
    {
      "title": "## 버그 수정 🐞",
      "labels": ["🐛bug"]
    },
    {
      "title": "## 기능 추가 및 개선 💫",
      "labels": ["💫enhancement"]
    },
    {
      "title": "## test ✅",
      "labels": ["✅test"]
    },
    {
      "title": "## 릴리즈 (Major) 📢",
      "labels": ["🚀release/major"]
    },
    {
      "title": "## 릴리즈 (Minor) 📢",
      "labels": ["🔥release/minor"]
    },
    {
      "title": "## 릴리즈 (Patch) 📢",
      "labels": ["🛠️release/patch"]
    },
    {
      "title": "## 릴리즈 (Library) 📢",
      "labels": ["🛰️release/library"]
    },
    {
      "title": "## 문서 작성 📝",
      "labels": ["📝docs"]
    },
    {
      "title": "## 질문 사항 ❓",
      "labels": ["❓question"]
    },
    {
      "title": "## 기타  🧪",
      "labels": []
    },
    {
      "title": "## 알파 버전 라이브러리 💊",
      "labels": ["💊trial"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "#{{CHANGELOG}}",
  "pr_template": "- #{{TITLE}}\n   - PR: [#{{NUMBER}}](#{{URL}})",
  "empty_template": "- no changes",
  "label_extractor": [],
  "reference": {},
  "transformers": [],
  "trim_values": false,
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": ["main"],
  "custom_placeholders": []
}